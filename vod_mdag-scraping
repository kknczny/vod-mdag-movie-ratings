from bs4 import BeautifulSoup
from urllib.request import urlopen
import ssl

def scrap_page(
    url = "https://vod.mdag.pl/pl/filmy",
    context = ssl.SSLContext()
):

    with urlopen(url, context=context) as response:
        soup = BeautifulSoup(response, "html.parser")
        titles = {}
        titles_eng = [value.string for value in soup.find_all("div", "moviebox-desc")]

        for key in soup.find_all("div", "moviebox-title"):
            for value in titles_eng:
                titles[key.string] = value
                titles_eng.remove(value)
                break
    return titles

titles = scrap_page()

